{
  "name": "cra-template",
  "version": "1.0.0",
  "scripts": {
    "build": "react-app-rewired build",
    "start": "GENERATE_SOURCEMAP=false react-app-rewired start",
    "test": "react-app-rewired test --colors",
    "coverage": "react-app-rewired test --coverage",
    "precommit": "lint-staged",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "prettier-format": "prettier --config .prettierrc src/**/*.tsx --write",
    "e2etest": "playwright test",
    "prepare": "husky install && cp -a .husky/. .git/hooks/"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "yarn prettier-format"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!**/*.d.ts?(x)",
      "!**/*.stories.ts?(x)",
      "!**/__*__/**"
    ]
  },
  "browserslist": {
    "production": [
      "chrome > 60",
      "firefox > 60",
      "safari > 12"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@chakra-ui/icons": "^2.0.11",
    "@chakra-ui/react": "^2.3.4",
    "@chakra-ui/styled-system": "^2.3.4",
    "@chakra-ui/system": "^2.2.12",
    "@chakra-ui/theme-tools": "^2.0.12",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mdx-js/react": "^2.1.3",
    "@saas-ui/react": "^1.4.3",
    "@saas-ui/sidebar": "^0.3.2",
    "@saas-ui/web3": "^1.1.0",
    "@terra-money/terra.js": "^3.0.2",
    "@terra-money/wallet-provider": "^3.6.5",
    "babel-plugin-macros": "^3.1.0",
    "css-loader": "^6.7.2",
    "date-fns": "^2.29.3",
    "eslint": "^8.24.0",
    "framer-motion": "^7.4.0",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-card-flip": "^1.1.5",
    "react-currency-input-field": "^3.6.5",
    "react-dom": "^18.2.0",
    "react-element-to-jsx-string": "^15.0.0",
    "react-icons": "^4.4.0",
    "react-inspector": "^6.0.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.1",
    "react-scroll": "^1.8.6",
    "recoil": "^0.7.2",
    "require-from-string": "^2.0.2",
    "sass": "^1.56.1",
    "typings-for-css-modules-loader": "^1.7.0",
    "yarn-deduplicate": "^6.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.19.2",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-scroll": "^1.8.3",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.4.4",
    "webpack": "5"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "main": "index.js",
  "repository": "https://github.com/Y-Foundry-Dao/yfd-dapp-gov.git",
  "author": "Ray Raspberry <ray@yfoundry.io>",
  "license": "MIT"
}
